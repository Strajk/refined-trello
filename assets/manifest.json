{
	"name": "Refined Trello",
	"version": "1.0.0",
	"description": "Simplifies the Trello interface and adds useful features",
	"homepage_url": "https://github.com/strajk/refined-trello",
	"manifest_version": 2,
	"minimum_chrome_version": "50",
	"permissions": [
		"https://trello.com/*"
	],
	"icons": {},
	"content_scripts": [
		{
			"run_at": "document_idle",
			"matches": [
				"https://trello.com/*"
			],
			"js": [
				"index.js"
			],
			"css": [
				"index.css"
			]
		}
	]
}
